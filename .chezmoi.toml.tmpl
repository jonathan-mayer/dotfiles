{{- $name := promptStringOnce . "name" "Full Name" -}}
{{- $email := promptStringOnce . "email" "Email address" -}}
{{- $go_bins := promptStringOnce . "go_bins" "Install Go Binaries? (y/n)" -}}

[data]
    name = {{ $name | quote }}
    email = {{ $email | quote }}
    go_bins = {{ $go_bins | quote }}
[git]
    autoCommit = true
